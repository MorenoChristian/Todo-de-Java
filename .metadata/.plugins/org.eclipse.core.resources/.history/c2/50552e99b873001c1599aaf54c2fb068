package Ej12;

public class Main {

	public static void main(String[] args) {
		
		Revolver revo = new Revolver();
		System.out.println(revo.toString());
		System.out.println(revo.dispara());
		revo.siguienteBala();
		System.out.println(revo.dispara());
		revo.siguienteBala();
		System.out.println(revo.dispara());
		revo.siguienteBala();
		System.out.println(revo.dispara());
		revo.siguienteBala();
		
		Jugador  jug = new Jugador("Christian");
		Jugador  jug1 = new Jugador("Christian");
		System.out.println(jug.getNombre());
		System.out.println(jug1.getNombre());
		
	}

}
